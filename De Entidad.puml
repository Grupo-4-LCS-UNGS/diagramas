  ABIERTO
  CERRADO_RESUELTO
  CERRADO_IMPOSIBLE_REPARAR
  EN_TALLER_EXTERNO
}

enum TipoMantenimiento {
  PREVENTIVO
  CORRECTIVO
  DAÃ‘O
}
@enduml
